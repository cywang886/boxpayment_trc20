{
  "name": "boxpayment/laravel",
  "description": "Gateway RTC20 USDT for Laravel package",
  "keywords": [
        "boxpayment",
        "tron",
        "erc20",
        "usdt",
        "laravel",
        "php"
   ],
  "license": "MIT",
  "homepage": "https://github.com/l3lackrooz/boxpayment-erc",
  "authors": [
    {
      "name": "Danial Rasouli",
      "email": "l3lackrooz@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/psr7": "^1.5",
    "illuminate/http": "^6.3|^7.0|^8.0",
    "illuminate/routing": "^6.3|^7.0|^8.0",
  },
  "require-dev": {
    "orchestra/testbench": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "l3lackrooz\\boxpayment-erc\\": "src/"
    }
  },
  "config": {
        "sort-packages": true
    },
 "extra": {
        "laravel": {
            "providers": [
                "l3lackrooz\\boxpayment-erc\\BoxPaymentServiceProvider"
            ],
        }
    }

  "minimum-stability": "dev"
}
